{
  "expo": {
    "name": "홈밥",
    "slug": "app-client",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo/logo.png",
    "scheme": "homebab",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0.1",
      "bundleIdentifier": "com.meowailand.homebab"
    },
    "web": {
      "favicon": "./assets/images/logo/logo.png"
    },
    "android": {
      "versionCode": 1,
      "package": "com.meowailand.homebab",
      "permissions": ["CAMERA"]
    }
  }
}
