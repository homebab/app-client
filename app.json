{
  "expo": {
    "name": "홈밥",
    "slug": "app-client",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/logo/home.png",
    "scheme": "homebab",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover"
    },
    "notification": {
      "icon": "./assets/images/logo/notification.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/images/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0.4",
      "bundleIdentifier": "com.meowailand.homebab",
      "userInterfaceStyle": "light"
    },
    "web": {
      "favicon": "./assets/images/logo/logo.png"
    },
    "android": {
      "versionCode": 1,
      "package": "com.meowailand.homebab",
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "CAMERA",
        "NOTIFICATION"
      ],
      "userInterfaceStyle": "light"
    }
  }
}